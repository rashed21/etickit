    <!--navigation part in 
    
    
    
    
    
    
    
    
    down-->
    
    
    
    <navigation-rule>
        <from-view-id>/*</from-view-id>
    <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/userPanel/index.xhtml</to-view-id>
            <redirect></redirect>
        </navigation-case>
        
           <navigation-case>
            <from-outcome>registrationSuccess</from-outcome>
            <to-view-id>/loginPage.xhtml</to-view-id>
            <redirect></redirect>
        </navigation-case>
        
          <navigation-case>
            <from-outcome>registrationFail</from-outcome>
            <to-view-id>/registration.xhtml</to-view-id>
            <redirect></redirect>
        </navigation-case>
    </navigation-rule>
